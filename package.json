{
  "name": "what-keystone",
  "version": "1.0.0",
  "description": "Discord bot for managing keystones in World of Warcraft",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "tsc": "tsc",
    "dev": "pnpm build && NODE_ENV=development node .",
    "start": "pnpm build && NODE_ENV=production node .",
    "start:prod": "npm run migrate:prod && NODE_ENV=production node .",
    "migrate:prod": "prisma migrate deploy",
    "generate": "prisma generate",
    "postinstall": "npm run generate"
  },
  "keywords": [
    "discord",
    "bot",
    "wow"
  ],
  "author": "Anthony Yarbrough <asyarb@hawaii.edu>",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^0.5.0",
    "@prisma/client": "^4.0.0",
    "common-tags": "^1.8.2",
    "discord-api-types": "^0.36.2",
    "discord.js": "^13.8.1",
    "jellycommands": "^1.0.0-next.19",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.11.0",
    "prettier": "^2.6.0",
    "prisma": "^4.0.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  }
}
