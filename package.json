{
  "name": "what-keystone",
  "version": "1.0.0",
  "description": "Discord bot for managing keystones in World of Warcraft",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsup",
    "tsc": "tsc",
    "dev": "pnpm build && NODE_ENV=development node .",
    "start": "pnpm build && NODE_ENV=production node .",
    "start:prod": "npm run migrate:prod && pnpm build && NODE_ENV=production node .",
    "migrate:prod": "prisma migrate deploy",
    "generate": "prisma generate",
    "postinstall": "npm run generate"
  },
  "keywords": [
    "discord",
    "bot",
    "wow"
  ],
  "author": "Anthony Yarbrough <asyarb@hawaii.edu>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "common-tags": "^1.8.2",
    "discord.js": "^13.6.0",
    "jellycommands": "^1.0.0-next.19",
    "kleur": "^4.1.4"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  }
}
